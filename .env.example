# === Binance API (Spot) ===
BINANCE_KEY=YOUR_BINANCE_API_KEY
BINANCE_SECRET=YOUR_BINANCE_SECRET
# Base URL (spot)
BINANCE_BASE=https://api.binance.com

# === App ===
PORT=8080
# Comma-separated allowed origins for CORS (e.g., https://my1.eltx.online)
ALLOWED_ORIGINS=https://my1.eltx.online,https://eltx.online
# If true, place LIMIT_MAKER instead of LIMIT (to avoid taker fees)
MAKER_ONLY=true
# Safety: max total USDT to allocate per symbol across active orders
MAX_SYMBOL_ALLOCATION_USDT=200
